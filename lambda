#lambda functions
#it does not have name
#multiple arguments but single return value
avg=lambda a,b:(a+b)/2
print(avg(10,30))

disct={'abc':123,'aad':112,'dot':111}
sorted(disct,key=lambda kv1:disct[kv1])
print(disct)
